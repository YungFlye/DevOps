{
  "name": "ci-demo",
  "version": "1.0.0",
  "description": "Demo project for demonstrating a CI pipeline",
  "main": "app.js",
  "scripts": {
    "test": "jest",
    "dev": "nodemon ./bin/www"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [],
  "author": "Lars Nysom",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.0.0",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.5.1",
    "supertest": "^6.3.4"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "globals": {
      "describe": "readonly",
      "test": "readonly",
      "expect": "readonly"
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "plugin:jest/recommended"
    ]
  }
}
